{
    "name": "partkeepr/partkeepr-ng",
    "description": "The Next Generation port of PartKeepr, the Open Source Inventory Software",
    "type": "project",
    "homepage": "http://www.partkeepr.org",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Timo A. Hummel",
            "email": "felicitus@felicitus.org",
            "homepage": "http://www.felicitus.org",
            "role": "Project Lead/Developer of PartKeepr"
        },
        {
            "name": "Jacob Litewski",
            "email": "jacoblitewski@gmail.com",
            "role": "Project Lead/Developer of PartKeepr-NG"
        },
        {
            "name": "Community contributors",
            "homepage": "https://github.com/jlitewski/PartKeepr-NG/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/jlitewski/PartKeepr-NG/issues",
        "irc": "irc://irc.freenode.org/partkeepr",
        "wiki": "http://wiki.partkeepr.org",
        "forum": "http://forum.partkeepr.org",
        "source": "https://github.com/jlitewski/PartKeepr-NG",
        "faq": "http://faq.partkeepr.org"
    },
    "require": {
        "php": ">=7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "^2.6",
        "atelierspierrot/famfamfam-silk-sprite": "^1.0",
        "brainbits/fugue-icons-bundle": "^3.5",
        "composer/package-versions-deprecated": "1.11.99.2",
        "doctrine/annotations": "^1.0",
        "doctrine/data-fixtures": "^1.5",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.9",
        "egulias/email-validator": "^2.1.10",
        "fortawesome/font-awesome": "^5.15",
        "friendsofsymfony/rest-bundle": "^3.0",
        "guzzlehttp/guzzle": "^7.3",
        "imagine/imagine": "^1.2",
        "incenteev/composer-parameter-handler": "^2.1",
        "knplabs/knp-gaufrette-bundle": "^0.7.1",
        "nelmio/api-doc-bundle": "^4.4",
        "nelmio/cors-bundle": "^2.1",
        "nucleos/user-bundle": "^1.11",
        "phpdocumentor/reflection-docblock": "^5.2",
        "phpseclib/phpseclib": "^3.0",
        "predis/predis": "^1.1",
        "ramsey/uuid": "^4.2",
        "sensio/framework-extra-bundle": "^6.1",
        "snc/redis-bundle": "^3.4",
        "sonata-project/exporter": "^2.7",
        "stof/doctrine-extensions-bundle": "^1.6",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-client": "4.4.*",
        "symfony/intl": "4.4.*",
        "symfony/mailer": "4.4.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/process": "4.4.*",
        "symfony/property-access": "4.4.*",
        "symfony/property-info": "4.4.*",
        "symfony/proxy-manager-bridge": "4.4.*",
        "symfony/security": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer": "4.4.*",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "^4.4",
        "symfony/validator": "4.4.*",
        "symfony/web-link": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.12",
        "symfony/yaml": "4.4.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "liip/functional-test-bundle": "^4.4",
        "phing/phing": "^2.16",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^4.4",
        "symfony/css-selector": "^4.4",
        "symfony/debug-bundle": "^4.4",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^5.3",
        "symfony/stopwatch": "^4.4",
        "symfony/var-dumper": "^4.4",
        "symfony/web-profiler-bundle": "^4.4"
    },
    "suggest": {
        "ext-curl": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.4.*"
        }
    }
}
